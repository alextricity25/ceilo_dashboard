<% content_for(:title) { "My super sweet dashboard" } %>
<script type='text/javascript'>
    $(function() {
    Dashing.gridsterLayout('[{"col":3,"row":2,"size_x":1,"size_y":1},{"col":3,"row":1,"size_x":1,"size_y":1},{"col":2,"row":1,"size_x":1,"size_y":2}]')
    });
</script>
<style>

.center
{
margin-left: auto; 
margin-right: auto; 
text-align: center; 
}

</style> 
<h1>Welcome to the Cloud :)</h1> 

    
<div class="gridster">
    <ul>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
            <div data-id="freedisk" data-view="Number" data-title="Total Disk Space Available" data-prefix="" data-moreinfo="In GB"></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
            <div data-id="numOfInstances" data-view="Number" data-title="Total Number of Instances " data-prefix=""></div>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
            <div data-id="computeNodes" data-view="List" data-title="VMs/Host" data-prefix="" data-moreinfo="" ></div>
        </li>


    </ul>
</div>
